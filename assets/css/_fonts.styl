@font-face
  font-family 'Source Sans Regular'
  src url('../fonts/source-sans/sourcesanspro-regular-webfont.eot')
  src url('../fonts/source-sans/sourcesanspro-regular-webfont.eot?#iefix') format('embedded-opentype'),
       url('../fonts/source-sans/sourcesanspro-regular-webfont.woff') format('woff'),
       url('../fonts/source-sans/sourcesanspro-regular-webfont.ttf') format('truetype'),
       url('../fonts/source-sans/sourcesanspro-regular-webfont.svg#webfont') format('svg');
@font-face
  font-family 'Source Sans Bold'
  src url('../fonts/source-sans/sourcesanspro-bold-webfont.eot')
  src url('../fonts/source-sans/sourcesanspro-bold-webfont.eot?#iefix') format('embedded-opentype'),
       url('../fonts/source-sans/sourcesanspro-bold-webfont.woff') format('woff'),
       url('../fonts/source-sans/sourcesanspro-bold-webfont.ttf') format('truetype'),
       url('../fonts/source-sans/sourcesanspro-bold-webfont.svg#webfont') format('svg');
